{
  "name": "miku-miku-crawler",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": {
    "name": "renbkna",
    "url": "https://github.com/renbkna"
  },
  "scripts": {
    "dev": "concurrently --ks SIGKILL -n \"Client,Server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "tsx server/server.js",
    "dev:client": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server/server.js",
    "postinstall": "npx puppeteer browsers install chrome && node -e \"const fs=require('fs'),path=require('path');const dir=path.join('.cache','puppeteer');if(!fs.existsSync(dir))fs.mkdirSync(dir,{recursive:true});\"",
    "postinstall:prod": "mkdir -p .cache/puppeteer",
    "test": "node --test"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.1",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "ipaddr.js": "^2.2.0",
    "lucide-react": "^0.554.0",
    "puppeteer": "^24.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "robots-parser": "^3.0.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sql.js": "^1.13.0",
    "sqlite3": "^5.1.7",
    "tsx": "^4.20.6",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2"
  }
}
