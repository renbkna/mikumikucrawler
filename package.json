{
  "name": "miku-miku-crawler",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": {
    "name": "renbkna",
    "url": "https://github.com/renbkna"
  },
  "scripts": {
    "dev": "concurrently --ks SIGKILL -n \"Client,Server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "tsx server/server.js",
    "dev:client": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server/server.js",
    "postinstall": "npx puppeteer browsers install chrome && node -e \"const fs=require('fs'),path=require('path');const dir=path.join('.cache','puppeteer');if(!fs.existsSync(dir))fs.mkdirSync(dir,{recursive:true});\"",
    "postinstall:prod": "mkdir -p .cache/puppeteer",
    "test": "node --test"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "ipaddr.js": "^2.2.0",
    "lucide-react": "^0.511.0",
    "puppeteer": "^24.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "robots-parser": "^3.0.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.2",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  }
}
